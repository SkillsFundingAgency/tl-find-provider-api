@page
@model Sfa.Tl.Find.Provider.Web.Pages.Provider.NotificationsModel
@{
    ViewData["Title"] = "Notifications";
}

@section breadcrumbs {
    <a asp-page="/Employer/EmployerList" class="govuk-back-link" id="tl-back">Return to employer list</a>
}

<div class="govuk-grid-row">
    <div class="govuk-grid-column-two-thirds">
        <h1 class="govuk-heading-xl">Notifications</h1>
    </div>
</div>

<div style="background: pink">
    <a asp-page="./AddNotification" class="govuk-link govuk-link--no-visited-state">Add notification page</a>
    <a asp-page="./EditNotification" class="govuk-link govuk-link--no-visited-state">Edit notification page</a>
    <a asp-page="./RemoveNotification" class="govuk-link govuk-link--no-visited-state">Remove notification page</a>
</div>
