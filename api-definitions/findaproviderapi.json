{
  "openapi": "3.0.1",
  "info": {
    "title": "T Levels Find a Provider Api",
    "version": "v3"
  },
  "paths": {
    "/api/v{version}/locations": {
      "get": {
        "tags": [
          "Locations"
        ],
        "summary": "Search for locations by partial name.",
        "operationId": "SearchLocations",
        "parameters": [
          {
            "name": "searchTerm",
            "in": "query",
            "description": "Search string.",
            "schema": {
              "type": "string"
            }
          },
          {
            "name": "version",
            "in": "path",
            "required": true,
            "schema": {
              "type": "string"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "Success",
            "content": {
              "text/plain": {
                "schema": {
                  "type": "array",
                  "items": {
                    "$ref": "#/components/schemas/Town"
                  }
                }
              },
              "application/json": {
                "schema": {
                  "type": "array",
                  "items": {
                    "$ref": "#/components/schemas/Town"
                  }
                }
              },
              "text/json": {
                "schema": {
                  "type": "array",
                  "items": {
                    "$ref": "#/components/schemas/Town"
                  }
                }
              }
            }
          }
        }
      }
    },
    "/api/v{version}/providers": {
      "get": {
        "tags": [
          "Providers"
        ],
        "summary": "Search for providers.",
        "operationId": "GetProviders",
        "parameters": [
          {
            "name": "searchTerm",
            "in": "query",
            "description": "Postcode that the search should start from.",
            "schema": {
              "type": "string"
            }
          },
          {
            "name": "lat",
            "in": "query",
            "description": "Latitude that the search should start from.",
            "schema": {
              "type": "number",
              "format": "double"
            }
          },
          {
            "name": "lon",
            "in": "query",
            "description": "Longitude that the search should start from.",
            "schema": {
              "type": "number",
              "format": "double"
            }
          },
          {
            "name": "routeId",
            "in": "query",
            "description": "Route ids to filter by. Optional, nulls or zeroes will be ignored.",
            "schema": {
              "type": "array",
              "items": {
                "type": "integer",
                "format": "int32"
              }
            }
          },
          {
            "name": "qualificationId",
            "in": "query",
            "description": "Qualification ids to filter by. Optional, nulls or zeroes will be ignored.",
            "schema": {
              "type": "array",
              "items": {
                "type": "integer",
                "format": "int32"
              }
            }
          },
          {
            "name": "page",
            "in": "query",
            "description": "Page to be displayed (zero-based).",
            "schema": {
              "maximum": 2147483647,
              "minimum": 0,
              "type": "integer",
              "format": "int32",
              "default": 0
            }
          },
          {
            "name": "pageSize",
            "in": "query",
            "description": "Number of items to return on a page.",
            "schema": {
              "maximum": 2147483647,
              "minimum": 1,
              "type": "integer",
              "format": "int32",
              "default": 5
            }
          },
          {
            "name": "version",
            "in": "path",
            "required": true,
            "schema": {
              "type": "string"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "Success",
            "content": {
              "text/plain": {
                "schema": {
                  "$ref": "#/components/schemas/ProviderSearchResponse"
                }
              },
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/ProviderSearchResponse"
                }
              },
              "text/json": {
                "schema": {
                  "$ref": "#/components/schemas/ProviderSearchResponse"
                }
              }
            }
          },
          "500": {
            "description": "Server Error"
          }
        }
      }
    },
    "/api/v{version}/qualifications": {
      "get": {
        "tags": [
          "Qualifications"
        ],
        "summary": "Returns a list of all qualifications.",
        "operationId": "GetQualifications",
        "parameters": [
          {
            "name": "version",
            "in": "path",
            "required": true,
            "schema": {
              "type": "string"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "Success",
            "content": {
              "text/plain": {
                "schema": {
                  "type": "array",
                  "items": {
                    "$ref": "#/components/schemas/Qualification"
                  }
                }
              },
              "application/json": {
                "schema": {
                  "type": "array",
                  "items": {
                    "$ref": "#/components/schemas/Qualification"
                  }
                }
              },
              "text/json": {
                "schema": {
                  "type": "array",
                  "items": {
                    "$ref": "#/components/schemas/Qualification"
                  }
                }
              }
            }
          },
          "404": {
            "description": "Not Found",
            "content": {
              "text/plain": {
                "schema": {
                  "$ref": "#/components/schemas/ProblemDetails"
                }
              },
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/ProblemDetails"
                }
              },
              "text/json": {
                "schema": {
                  "$ref": "#/components/schemas/ProblemDetails"
                }
              }
            }
          }
        }
      }
    },
    "/api/v{version}/routes": {
      "get": {
        "tags": [
          "Routes"
        ],
        "summary": "Returns a list of all routes.",
        "operationId": "GetRoutes",
        "parameters": [
          {
            "name": "version",
            "in": "path",
            "required": true,
            "schema": {
              "type": "string"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "Success",
            "content": {
              "text/plain": {
                "schema": {
                  "type": "array",
                  "items": {
                    "$ref": "#/components/schemas/Route"
                  }
                }
              },
              "application/json": {
                "schema": {
                  "type": "array",
                  "items": {
                    "$ref": "#/components/schemas/Route"
                  }
                }
              },
              "text/json": {
                "schema": {
                  "type": "array",
                  "items": {
                    "$ref": "#/components/schemas/Route"
                  }
                }
              }
            }
          },
          "404": {
            "description": "Not Found",
            "content": {
              "text/plain": {
                "schema": {
                  "$ref": "#/components/schemas/ProblemDetails"
                }
              },
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/ProblemDetails"
                }
              },
              "text/json": {
                "schema": {
                  "$ref": "#/components/schemas/ProblemDetails"
                }
              }
            }
          }
        }
      }
    }
  },
  "components": {
    "schemas": {
      "DeliveryYearSearchResult": {
        "type": "object",
        "properties": {
          "year": {
            "type": "integer",
            "format": "int32"
          },
          "isAvailableNow": {
            "type": "boolean"
          },
          "qualifications": {
            "type": "array",
            "items": {
              "$ref": "#/components/schemas/Qualification"
            },
            "nullable": true,
            "deprecated": true
          },
          "routes": {
            "type": "array",
            "items": {
              "$ref": "#/components/schemas/Route"
            },
            "nullable": true
          }
        },
        "additionalProperties": false
      },
      "ProblemDetails": {
        "type": "object",
        "properties": {
          "type": {
            "type": "string",
            "nullable": true
          },
          "title": {
            "type": "string",
            "nullable": true
          },
          "status": {
            "type": "integer",
            "format": "int32",
            "nullable": true
          },
          "detail": {
            "type": "string",
            "nullable": true
          },
          "instance": {
            "type": "string",
            "nullable": true
          }
        },
        "additionalProperties": {}
      },
      "ProviderSearchResponse": {
        "type": "object",
        "properties": {
          "error": {
            "type": "string",
            "nullable": true
          },
          "searchTerm": {
            "type": "string",
            "nullable": true
          },
          "totalResults": {
            "type": "integer",
            "format": "int32",
            "nullable": true
          },
          "searchResults": {
            "type": "array",
            "items": {
              "$ref": "#/components/schemas/ProviderSearchResult"
            },
            "nullable": true
          }
        },
        "additionalProperties": false
      },
      "ProviderSearchResult": {
        "type": "object",
        "properties": {
          "providerName": {
            "type": "string",
            "nullable": true
          },
          "postcode": {
            "type": "string",
            "nullable": true
          },
          "town": {
            "type": "string",
            "nullable": true
          },
          "email": {
            "type": "string",
            "nullable": true
          },
          "telephone": {
            "type": "string",
            "nullable": true
          },
          "website": {
            "type": "string",
            "nullable": true
          },
          "distance": {
            "type": "number",
            "format": "double"
          },
          "deliveryYears": {
            "type": "array",
            "items": {
              "$ref": "#/components/schemas/DeliveryYearSearchResult"
            },
            "nullable": true
          }
        },
        "additionalProperties": false
      },
      "Qualification": {
        "type": "object",
        "properties": {
          "id": {
            "type": "integer",
            "format": "int32"
          },
          "name": {
            "type": "string",
            "nullable": true
          }
        },
        "additionalProperties": false
      },
      "Route": {
        "type": "object",
        "properties": {
          "id": {
            "type": "integer",
            "format": "int32"
          },
          "name": {
            "type": "string",
            "nullable": true
          },
          "numberOfQualifications": {
            "type": "integer",
            "format": "int32",
            "readOnly": true
          },
          "numberOfQualificationsOffered": {
            "type": "integer",
            "format": "int32"
          },
          "qualifications": {
            "type": "array",
            "items": {
              "$ref": "#/components/schemas/Qualification"
            },
            "nullable": true
          }
        },
        "additionalProperties": false
      },
      "Town": {
        "type": "object",
        "properties": {
          "id": {
            "type": "integer",
            "format": "int32"
          },
          "name": {
            "type": "string",
            "nullable": true
          },
          "county": {
            "type": "string",
            "nullable": true
          },
          "la": {
            "type": "string",
            "nullable": true
          }
        },
        "additionalProperties": false
      }
    }
  }
}